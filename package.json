{
  "name": "@beluga-labs/provider-stack",
  "version": "0.2.1",
  "type": "module",
  "description": "Compose React providers into a clean, configurable stack",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "example": "cd example && vite",
    "prepublishOnly": "pnpm build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/beluga-labs/provider-stack"
  },
  "keywords": [
    "react",
    "react-provider",
    "provider",
    "context",
    "composition",
    "nextjs",
    "remix",
    "gatsby",
    "vite",
    "create-react-app",
    "react-framework"
  ],
  "bugs": {
    "url": "https://github.com/beluga-labs/provider-stack/issues"
  },
  "author": "beluga labs",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.0.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rollup": "^4.53.5",
    "rollup-plugin-dts": "^6.3.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^5.0.0",
    "vitest": "^3.0.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "packageManager": "pnpm@10.20.0"
}
